{
  "name": "feijoa-ui",
  "version": "0.0.0",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:hamlim/feijoa-ui.git",
  "author": "Matt Hamlin <matthewjameshamlin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "babel src/ --out-dir dist/ --extensions '.ts,.tsx' && tsc",
