    "./*": {
      "import": "./dist/*",
      "require": "./dist/*",
      "types": "./dist/*.d.ts"
    },
    "./dist/*.js": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
